<div class="mt-3">
  <h1 class="text-2xl font-bold mb-5">組織修改</h1>
  <%= form_with model: @team, url: teams_path do |form| %>
    <div class="mb-3">
      <%= form.label :name, "組織名稱" %>
      <%= form.text_field :name %><br/>
    </div>
    <%= form.submit "更新", class:"btn-dashboard-create" %>
  <% end %>
</div>
